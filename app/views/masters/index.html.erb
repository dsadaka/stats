<h1>Listing masters</h1>

<table>
  <thead>
    <tr>
      <th>Playerid</th>
      <th>Birthyear</th>
      <th>Namefirst</th>
      <th>Namelast</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @masters.each do |master| %>
      <tr>
        <td><%= master.playerID %></td>
        <td><%= master.birthYear %></td>
        <td><%= master.nameFirst %></td>
        <td><%= master.nameLast %></td>
        <td><%= link_to 'Show', master %></td>
        <td><%= link_to 'Edit', edit_master_path(master) %></td>
        <td><%= link_to 'Destroy', master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Master', new_master_path %>
