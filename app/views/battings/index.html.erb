<h1>Listing battings</h1>

<table>
  <thead>
    <tr>
      <th>Playerid</th>
      <th>Yearid</th>
      <th>League</th>
      <th>Teamid</th>
      <th>G</th>
      <th>Ab</th>
      <th>R</th>
      <th>H</th>
      <th>Twob</th>
      <th>Threeb</th>
      <th>Hr</th>
      <th>Rbi</th>
      <th>Sb</th>
      <th>Cs</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @battings.each do |batting| %>
      <tr>
        <td><%= batting.playerID %></td>
        <td><%= batting.yearID %></td>
        <td><%= batting.league %></td>
        <td><%= batting.teamID %></td>
        <td><%= batting.G %></td>
        <td><%= batting.AB %></td>
        <td><%= batting.R %></td>
        <td><%= batting.H %></td>
        <td><%= batting.twoB %></td>
        <td><%= batting.threeB %></td>
        <td><%= batting.HR %></td>
        <td><%= batting.RBI %></td>
        <td><%= batting.SB %></td>
        <td><%= batting.CS %></td>
        <td><%= link_to 'Show', batting %></td>
        <td><%= link_to 'Edit', edit_batting_path(batting) %></td>
        <td><%= link_to 'Destroy', batting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Batting', new_batting_path %>
