<h1>Listing pitchings</h1>

<table>
  <thead>
    <tr>
      <th>Playerid</th>
      <th>Yearid</th>
      <th>League</th>
      <th>Teamid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pitchings.each do |pitching| %>
      <tr>
        <td><%= pitching.playerID %></td>
        <td><%= pitching.yearID %></td>
        <td><%= pitching.league %></td>
        <td><%= pitching.teamID %></td>
        <td><%= link_to 'Show', pitching %></td>
        <td><%= link_to 'Edit', edit_pitching_path(pitching) %></td>
        <td><%= link_to 'Destroy', pitching, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pitching', new_pitching_path %>
